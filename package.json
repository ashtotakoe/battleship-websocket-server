{
  "name": "ws_task",
  "version": "1.0.0",
  "description": "Template for the WebSocket remote control task",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "front:start": "node dist/index.js",
    "start:dev": "tsx watch --no-warnings src/main.ts",
    "start:prod": "tsc && node dist/main.js",
    "build": "tsc",
    "eslint": "eslint \"src/**/*.ts\"",
    "eslint:fix": "eslint \"src/**/*.ts\" --fix",
    "prettier": "prettier \"src/**/*.{ts,js}\" --check",
    "prettier:write": "prettier \"src/**/*.{ts,js}\" --write"
  },
  "author": "Andrei Auchynnikau",
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "@types/node": "^20.11.18",
    "@types/ws": "^8.5.10",
    "rxjs": "^7.8.1",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3",
    "ws": "^8.16.0"
  }
}
